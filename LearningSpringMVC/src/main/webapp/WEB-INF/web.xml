<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>dad-front-controller-dispatcher</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>ground-front-controller-dispatcher</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>dad-front-controller-dispatcher</servlet-name>
    <url-pattern>/home.com/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ground-front-controller-dispatcher</servlet-name>
    <url-pattern>/ground.com/*</url-pattern>
  </servlet-mapping>
  
</web-app>
